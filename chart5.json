{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 200,  
  "background": "null",
  "data": {
    "url": "https://raw.githubusercontent.com/nishagayathri9/Heritage_Sites/main/beeswarm.csv",
    "format": {"type": "csv"}
  },
  "mark": "area",
  "transform": [
    {
      "density": "Rating",
      "groupby": ["category"],
      "extent": [0, 5]
    }
  ],
  "encoding": {
    "x": {
      "field": "value",
      "type": "quantitative",
      "title": "Rating",
      "axis": {
        "labelColor": "white",
        "titleColor": "white",
        "gridColor": "white",
        "tickColor": "white"
      }
    },
    "y": {
      "field": "density",
      "type": "quantitative",
      "stack": "zero",
      "axis": {
        "labelColor": "white",
        "titleColor": "white",
        "gridColor": "white",
        "tickColor": "white"
      }
    },
    "color": {
      "field": "category",
      "type": "nominal",
      "scale": {
        "range": ["#2C0E37", "#DBBADD", "#CB429F"]
      },
      "title": "Category",
      "legend": {
        "labelColor": "white",
        "titleColor": "white"
      }
    }
  }
}
